/* pages/tostore/tostore.wxss */
@import "../qrcode/qrcode.wxss";
@import "../Turnoverrate/Turnoverrate.wxss";
@import "../Pusheffect/Pusheffect.wxss";
.swiper-tab-list{width:33.33%;}
.qrcode{width:100%;display: flex;flex-direction: column;padding: 20rpx 40rpx;box-sizing:border-box;}
.qrcode image{width:100%;height:600rpx;}
.Choice{width:100%;display: flex;flex-direction: column;padding:20rpx 40rpx;box-sizing: border-box;}
.icon{width:100%;display: flex;flex-direction: row;}
.today{width:100%;font-size:28rpx;}
.radio-group{width:100%;display: flex;flex-direction: row;}
.radio{width:33.33%;padding:20rpx 0rpx;}
textarea{font-size:28rpx;border:2rpx solid #ccc;width:100%;padding:10rpx 20rpx;box-sizing:border-box;}
.submit{font-size:28rpx;color:#fff;background:#4cd964;width:60%;margin:20rpx auto;text-align:center;padding:20rpx 0rpx;border-radius:10rpx;}





























