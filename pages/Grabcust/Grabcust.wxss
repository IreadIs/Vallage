/* pages/Grabcust/Grabcust.wxss */
.workbg{width:100%;height:176rpx;display:flex;flex-direction: column;background:url(https://qiyeplus.qiyeplus.com/data/upload/forsmall/images/bannerbg1.png) center center no-repeat}
.workbg .mine{padding:20rpx 40rpx;box-sizing:border-box;font-size:36rpx;color:#fff;}
.worknavclass{width:100%;text-align:right;}
.icongold{width:100%;display: flex;flex-direction: row;padding: 20rpx 0rpx;}
.places{width:50%;display: flex;flex-direction: column;text-align: center;border-bottom:2rpx solid #ccc;padding-bottom:20rpx;}
.tx1{color:#ff9500;font-size:40rpx;border-right:1px solid #ccc}
.tx2{font-size:28rpx;color:#333;border-right:1px solid #ccc}
.item{border-bottom:2rpx solid #ccc;width:100%;display: flex;flex-direction: row;padding: 10rpx 20rpx;box-sizing:border-box;}
.item image{width:100rpx;height:100rpx;border-radius:50%;}
.text{flex:1;display: flex;flex-direction: column;padding:10rpx 0rpx 0rpx 20rpx;}
.title{font-size:28rpx;}
.description{font-size:24rpx;color:#8e8e93;}
.containerts{width:100%;}
.jdtiao{line-height:100rpx;padding-right:20rpx;}
.jinzi{background:orange;padding:5px 10px;border-radius:5px;color:#fff;font-size:28rpx;}
.golds{padding-top:20rpx;}
progress{width:200rpx;padding-right:20rpx;}

















